{
  "extends": "next/core-web-vitals",
  "rules": {
    "quotes": ["error", "single", { "avoidEscape": true }],
    "no-unused-vars": "off",
    "max-len": [
      "error",
      {
        "code": 150,
        "ignoreComments": true,
        "ignoreTrailingComments": true,
        "ignoreUrls": true,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true
      }
    ]
  },
  "env": {},
  "overrides": []
}
