{
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "singleQuote": true,
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "semi": true,
  "trailingComma": "es5",
  "printWidth": 120,
  "proseWrap": "always",
  "quoteProps": "as-needed",
  "tabWidth": 2,
  "useTabs": false
}
